<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>SimpleSimd</RootNamespace>
    <nullable>enable</nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Gilad Freidkin</Authors>
    <RepositoryUrl>https://github.com/giladfrid009/SimpleSIMD</RepositoryUrl>
    <PackageTags>SIMD, Library, Performance, Parallel, Math, Generic, Allocation</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Easy to use SIMD accelerated array extensions</Description>
    <Copyright>Copyright 2020 (c) Gilad Freidkin, All rights reserved</Copyright>
    <Version>2.0.1</Version>
    <PackageReleaseNotes>Added Span support</PackageReleaseNotes>
  </PropertyGroup>

</Project>
